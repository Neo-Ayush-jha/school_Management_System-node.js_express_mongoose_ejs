      <div class="card card-outline card-primary">
        <div class="card-header text-center">
          <a href="" class="h1"><b>Student</b> apply</a>
        </div>
        <p class="login-box-msg">Register a new membership</p>
        <div class="card-body py-0 my-0">


          <form action="" method="post", enctype="multipart/form-data" py-0 my-0>
            <div class="card-body">
              <div class="row">

                <div class="form-group col">
                  <label for="">First Name</label>
                  <input type="text" , name="first_name" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Admission fee">
                </div>
                <div class="form-group col">
                  <label for="">Last Name</label>
                  <input type="text" , name="last_name" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Admission fee">
                </div>
              </div>
              <div class="row">
                <div class="form-group col">
                  <label for="">Father Name</label>
                  <input type="text" , name="father_name" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Monthly fee">
                </div>
                <div class="form-group col">
                  <label for="">Mother Name</label>
                  <input type="text" , name="mother_name" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Extera fee">
                </div>
              </div>
              <div class="form-group col">
                <label for="">Image</label>
                <input type="file" , name="image" class="form-control" 
                    placeholder="Pleace enter Class image">
              </div>
              <div class="row">
                <div class="form-group col">
                  <label for="">DOB</label>
                  <input type="date" , name="dob" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Extera fee">
                </div>
                <div class="mb-3 col">
                  <label for="">Course</label>
                  <select name="course_id" , id="" class="form-control">
                    <% course.forEach(val=>{ %>
                      <!-- <td><%=val.address %></td> -->
                      <option value="<%= val._id  %>">
                        <%=val.Course_name %>
                      </option>
                      <% }); %>
                  </select>

                </div>
              </div>
              <div class="row">
                <div class="form-group col">
                  <label for="">mobile</label>
                  <input type="number" , name="mobile" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Extera fee">
                </div>
                <div class="form-group col">
                  <label for="">email</label>
                  <input type="email" , name="email" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Books fee">
                </div>
              </div>
              <div class="row">
                <div class="form-group col">
                  <label for="">Gender</label>
                  <div class="mb-1 col px-3 rounded py-1 d-flex border border-1">
                    <div class="mx-3">
                      <input type="radio" name="gender" value="male" class="me-5 form-check-input me-1">
                      <label for="" class="me-5 form-check-label">Male</label>
                    </div>
                    <div class="mx-3">
                      <input type="radio" name="gender" value="female" class="me-5 form-check-input me-1">
                      <label for="" class="me-5 form-check-label">Female</label>
                    </div>
                    <div class="mx-3">
                      <input type="radio" name="gender" value="other" class="me-5 form-check-input me-1">
                      <label for="" class="me-5 form-check-label">Other</label>
                    </div>
                  </div>
                </div>
                <div class="form-group col">
                  <label for="">password</label>
                  <input type="password" , name="password" class="form-control" id="exampleInputPassword1"
                    placeholder="Class Books fee">
                </div>
              </div>
              <!--------------------- ----------------------------------------------------- -->


              <!--------------------- ----------------------------------------------------- -->

              <div class="form-group col">
                <label for="">address</label>
                <textarea name="address" id="exampleInputPassword1" class="form-control" placeholder="Class Extera fee"
                  cols="30" rows=""></textarea>
              </div>

            </div>
            <!-- /.card-body -->

            <div class="card-footer ">
              <button type="submit" class="btn btn-primary w-100">Submit</button>
            </div>
          </form>

          <div class="row social-auth-links text-center">
            <div class="col"><a href="#" class="btn btn-block btn-primary">
