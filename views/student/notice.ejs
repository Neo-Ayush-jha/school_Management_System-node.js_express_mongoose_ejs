                                          <a href="#"><%= val.teacher_id.t_name %></a>
                                        </span>
                                        <span class="description">Shared publicly - <%= val.date %></span>
                                      </div>
                                      <!-- /.user-block -->
                                      
                                      <p class="mt-5"> 
                                        <%= val.notice %>
                                       
                                      </p>
                                   
                                      <p>
                                        <a href="#" class="link-black text-sm"><i class="fas fa-link mr-1"></i> Dem  o File 1 v2</a>
                                      </p>
                                    <!-- </div> -->
                                  </div>
                                </div>
            
                                <% }); %>
                                
                            </div>
                          </div>
                        </div>
                        <div class="col-12 col-md-12 col-lg-4 order-1 order-md-2">
                          <h3 class="text-primary"><i class="fas fa-paint-brush"></i> Online School</h3>
