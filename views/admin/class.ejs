<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>hello</title>
    <%- include('./layout/common/link.ejs')%>
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <%- include('./layout/common/header.ejs') %>
            <%- include('./layout/common/nav.ejs') %>
                <div class="content-wrapper">



                    <div class="card card-default">
                        <div class="card-header">
                            <h3 class="card-title">Class Room's</h3>

                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                            <div class="row p-2">
                                <% course.forEach(val=>{ %>
                                    <div class="col-3">
                                        <div class="card bg-info">
                                            <div class="card-body">
                                                <h1 class="  text-center"><%= val.Course_name%></h1>
                                            <h3 class="text-center"><a href="/class_room/<%= val._id%>" class="btn btn-success ">OPEN </a></h3>
                                        </div>
                                    </div>
                                </div>
                                <% }); %> 
                            </div>
                        <!-- /.card-body -->
                        
                    </div>
                </div>
                <!-- <%- include('./layout/common/footer.ejs') %> -->
    </div>
    <%- include('./layout/common/downlink.ejs') %>
</body>

</html>