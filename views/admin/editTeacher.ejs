                  </div>
                  <!-- /.form-group -->
                </div>
                <div class="row">
                  <div class="col-12 col-sm-6">
                    <div class="form-group">
                      <label for="">gender</label>
                      <input type="text" , name="t_gender" , value="<%= teacher[0].t_gender %>" class="form-control">
