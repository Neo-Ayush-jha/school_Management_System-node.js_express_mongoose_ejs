                        <input type="password" , name="t_password" , value=" <%=teacher[0].t_password %> " class="form-control">

                    </div>
                  </div>
                  <!-- /.form-group -->
                </div>
                <div class="mb-3">
                  <input type="submit" , name="submit" class="btn btn-success w-100">
                </div>
                <!-- /.col -->
              
            <!-- /.row -->
            </form>
