                    <!-- /.form-group -->
                  </div>
                  <!-- /.col -->
                  <div class="col-12 col-sm-6">
                    <div class="form-group">
                      <label for="">Course</label>
                      <input type="text" , name="course_id" , value="<%= teacher[0].course_id %>"
                        class="form-control">
                    </div>
                  </div>
                  <!-- /.form-group -->
                </div>
                <div class="row">
                  <div class="col-12 col-sm-6">
                    <div class="form-group">
                      <label for="">Address</label>
                      <input type="text" , name="t_address" , value=" <%=teacher[0].t_address %> " class="form-control">
