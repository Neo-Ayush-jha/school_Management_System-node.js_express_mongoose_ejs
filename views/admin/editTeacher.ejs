              </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <form action="" method="post">
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="">Teacher name</label>
                      <input type="text" , name="t_name" , value=" <%=teacher[0].t_name %> " class="form-control">
                    </div>
                    <!-- /.form-group -->
                    <div class="form-group">
                      <label for="">Mobile</label>
                      <input type="number" , name="t_mobile" , value="<%= teacher[0].t_mobile %>" class="form-control">
                    </div>
                    <!-- /.form-group -->
                  </div>
                  <!-- /.col -->
                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="">Age</label>
                      <input type="number" , name="t_age" , value="<%= teacher[0].t_age %>" class="form-control">
                    </div>
                    <!-- /.form-group -->
                    <div class="form-group">
                      <label for="">email</label>
                      <input type="email" , name="t_email" , value="<%= teacher[0].t_email %>" class="form-control">
                      </select>
                    </div>
                    <!-- /.form-group -->
                  </div>
                  <!-- /.col -->
                </div>
                <!-- /.row -->

                <h5>Custom Color Variants</h5>
                <div class="row">
                  <div class="col-12 col-sm-6">
                    <div class="form-group">
                      <label for="">Salary</label>
                      <input type="number" , name="t_salary" , value="<%= teacher[0].t_salary %>" class="form-control">
                    </div>
                    <!-- /.form-group -->
                  </div>
                  <!-- /.col -->
                  <div class="col-12 col-sm-6">
                    <div class="form-group">
                      <label for="">Dob</label>
                      <input type="text" , name="t_dob" , value="<%= teacher[0].t_dob %>" class="form-control">
