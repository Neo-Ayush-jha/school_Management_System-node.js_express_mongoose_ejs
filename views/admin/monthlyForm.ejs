<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>hello</title>
    <%- include('./layout/common/link.ejs')%>
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <%- include('./layout/common/header.ejs') %>
            <%- include('./layout/common/nav.ejs') %>
                <div class="content-wrapper">



                    <div class="card card-default">
                        <div class="card-header">
                            <h3 class="card-title">Asset Entry</h3>

                            <div class="card-tools">
                                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-tool" data-card-widget="remove">
                                    <i class="fas fa-times"></i>
                                </button>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <form action="" method="post">
                                <div class="row">
                                    <div class="mb-3 col">
                                        <label for="">student</label>
                                        <select name="student_id", id="" class="form-select">
                                            <% student.forEach(val=>{ %>
                                                        <!-- <td><%=val.address %></td> -->
                                                <option value="<%= val._id  %>"><%=val.first_name %></option>
                                            <% }); %> 
                                        </select>
                
                                    </div>
                                    <div class="mb-3 col">
                                        <label for="">student</label>
                                        <select name="course_id", id="" class="form-select">
                                            <% course.forEach(val=>{ %>
                                                        <!-- <td><%=val.address %></td> -->
                                                <option value="<%= val._id  %>"><%=val.Course_name %></option>
                                            <% }); %> 
                                        </select>
                
                                    </div>
                                    
                                </div>



                                <div class="mb-3">
                                    <input type="submit" , name="submit" class="btn btn-success w-100">
                                </div>
                                <!-- /.col -->

                                <!-- /.row -->
                            </form>
                        </div>
                        <!-- /.card-body -->
                        <div class="card-footer">
                            Visit <a href="https://select2.github.io/">Select2 documentation</a> for more examples and
                            information about
                            the plugin.
                        </div>
                    </div>
                </div>
                <%- include('./layout/common/footer.ejs') %>
    </div>
    <%- include('./layout/common/downlink.ejs') %>
</body>

</html>